{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:vite": "vite",
    "dev:type-predicates-generator": "type-predicates-generator -f 'src/types/routes.ts' -o 'src/utils/predicates.ts' -a -w --default-array-check-option first -c --whitelist",
    "dev": "yarn apigen && run-p 'dev:*'",
    "apigen": "./bin/generate_api.sh",
    "typecheck": "tsc -p . --noEmit",
    "lint:eslint": "eslint src --cache --ext .ts,.tsx",
    "lint:stylelint": "stylelint --cache 'src/styles/**/*.{css,scss}'",
    "lint:prettier": "prettier '**/*.{ts,tsx,js,json,css,scss,yml,yaml}' --check",
    "lint": "run-p 'lint:*'",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:stylelint": "yarn lint:stylelint --fix",
    "fix:prettier": "prettier '**/*.{ts,tsx,js,json,css,scss,yml,yaml}' --write",
    "fix": "run-p 'fix:*'",
    "npm-watch": "npm-watch",
    "build:typecheck": "yarn typecheck",
    "build:vite": "vite build",
    "build": "run-s 'build:*'",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy:build": "yarn build",
    "deploy:fb": "firebase deploy --only hosting",
    "deploy": "run-s 'deploy:*'",
    "postinstall:husky": "husky install",
    "postinstall:git": "git config --local --add merge.ff false && git config --local --add pull.ff only",
    "postinstall": "run-p 'postinstall:*'"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@aspida/axios": "^1.9.1",
    "@aspida/swr": "^1.9.1",
    "@tanstack/react-location": "^3.7.4",
    "aspida": "^1.9.1",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "rambda": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.29.0",
    "react-icons": "^4.4.0",
    "swr": "^1.2.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-docs": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-vite": "^0.1.33",
    "@storybook/jest": "^0.0.10",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.2.4",
    "@testing-library/react": "^13.3.0",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.2",
    "commitizen": "^4.2.4",
    "cssnano": "^5.1.7",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-storybook": "^0.5.11",
    "firebase-tools": "^11.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "modern-css-reset": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "openapi2aspida": "^0.19.0",
    "postcss": "^8.4.12",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.4.3",
    "prettier": "^2.6.1",
    "recoil": "^0.7.0",
    "sass": "^1.49.11",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.2.0",
    "type-fest": "^2.13.1",
    "type-predicates-generator": "^0.3.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.0",
    "vitest": "^0.11.0"
  },
  "packageManager": "yarn@3.0.0"
}
